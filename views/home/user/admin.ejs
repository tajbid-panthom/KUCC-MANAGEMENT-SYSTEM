<% layout("/layouts/boilerplate.ejs") %>
<div class="row justify-content-center">
  <div class="col-md-8 col-lg-6">
    <div class="mb-2">
      <div
        class="d-inline-block bg-white p-3 rounded-circle shadow-sm mb-1 text-center"
      >
        <a href="/">
          <img
            src="/images/logoMain.jpg"
            alt="Career Club Logo"
            class="img-fluid"
            style="
              width: 100px;
              height: 100px;
              object-fit: cover;
              border-radius: 50%;
            "
          />
        </a>
      </div>
    </div>
    <div class="mb-3 bg-white p-3 rounded shadow-sm text-center">
      <%=admin.name %>
    </div>

    <div class="mb-3 bg-white p-3 rounded shadow-sm text-center">
      <%=admin.email %>
    </div>
  </div>
</div>
